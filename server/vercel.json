{
    "version": 2,
    "builds": [{
      "src": "./index.jsx",
      "use": "@now/node-server"
    }],
    "routes": [
      {"handle": "filesystem"}, 
      {
        "src": "/(.*)", 
        "dest": "/route/routes.jsx" 
      },
      {
        "src": "/(.*)",
        "dest": "./index.jsx"
      }
    ]
  }
  